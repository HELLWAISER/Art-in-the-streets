	function abrirDica(pergunta){
		var arrayLength = perguntas.length;
		for (var i = 0; i < arrayLength; i++) {
			if ( perguntas[i] == pergunta ){
				var dica = dicas[i];
				dica = dica.replace("\n\n", "<br><br>");
				dica = dica.replace("\n\n", "<br><br>");
				dica = dica.replace("\n\n", "<br><br>");
				dica = dica.replace("\n\n", "<br><br>");
				
				// Tamanho dinâmico do modal de acordo com o tamanho da dica
				if ( dica.length < 371 ){
					document.getElementById("dicaModal").style.top = '10%';
					document.getElementById("dicaModal").style.height = '300px';
				}else if ( dica.length < 421 ){
					document.getElementById("dicaModal").style.top = '10%';
					document.getElementById("dicaModal").style.height = '380px';
				}else{
					if ( window.innerHeight < 600 ){
						document.getElementById("dicaModal").style.top = '10%';
						document.getElementById("dicaModal").style.height = '480px';
					}else{
						document.getElementById("dicaModal").style.top = '10%';
						document.getElementById("dicaModal").style.height = '480px';
					}

				}
				document.getElementById("textoDicaPerguntaMobile").innerHTML = dica;
			}
		}
		document.getElementById("fundoDicaMobile").className = '';
		document.getElementById("dicaModal").className = 'modal-content-rapido';
	}

	function fechaDica(){
		document.getElementById("fundoDicaMobile").className = 'hideMe';
		document.getElementById("dicaModal").className = 'modal-content-rapido hideMe';
	}
	
