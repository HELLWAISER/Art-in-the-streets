					data: {pergunta: pergunta_esconder, pergunta2: proxima_pergunta, tipoContrato: window.location.href},
					type: 'POST',
					success: function(output) {
						swal("Oops...", "Ocorreu um erro ao abrir a próxima pergunta. Um e-mail foi enviado para o nosso time técnico.", "error");
					}
		});
	}	
	
}


