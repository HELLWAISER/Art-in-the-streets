function alteraTamanhoInput(idInput) {
	var stringInputada = document.getElementById(idInput).value;

	if ( stringInputada.length < 1 ){
		var stringPlaceHolder = document.getElementById(idInput).placeholder;
		var tamanhoInput = retornaTamanhoStringPixels(stringPlaceHolder, idInput);
		if (document.getElementById(idInput).className == 'textAreaPrevia' ){
			document.getElementById(idInput).style.width = '100%';
		}else{
			document.getElementById(idInput).style.width = tamanhoInput;
		}
	}else{	
		var tamanhoInput = retornaTamanhoStringPixels(stringInputada, idInput);
		if (document.getElementById(idInput).className == 'textAreaPrevia' ){
			document.getElementById(idInput).style.width = '100%';
		}else{
			document.getElementById(idInput).style.width = tamanhoInput + 2;
		}
	}
}

