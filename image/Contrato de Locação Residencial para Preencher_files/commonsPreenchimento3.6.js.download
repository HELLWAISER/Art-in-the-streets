						success: function(output) {
						}
			});
			// Grava uma linha somente
			i = arr_temporario.length + 1;
		}
		// Para tirar todas as perguntas abertas, somente mantendo a pergunta atual, 
		// evitando assim definitivamente ter 2 modais abertos.
		div_esconder = document.getElementById(arr_temporario[i]);
		div_esconder.className = 'modal-content hideMe';
	}


	try{
		verificaDicaPergunta(proxima_pergunta);
	}catch(err) {
	}
	
	
	var cusid_ele = document.getElementsByClassName('modal-content-rapido');
	var modalsAbertos = 0;
	var ultimaPergunta = '';
	
	for (var i = 0; i < cusid_ele.length; ++i) {
		var item = cusid_ele[i];  
		if ( item.className.indexOf('hideMe') !== -1 ){
		}else{
			modalsAbertos = modalsAbertos + 1;
			ultimaPergunta = item.id;
		}
	}
	
	cusid_ele = document.getElementsByClassName('modal-content');
	for (var i = 0; i < cusid_ele.length; ++i) {
		var item = cusid_ele[i];  
		if ( item.className.indexOf('hideMe') !== -1 ){
		}else{
			modalsAbertos = modalsAbertos + 1;
			ultimaPergunta = item.id;
		}
	}
	
	if ( modalsAbertos > 1 ){
		// Grava em arquivo TXT o erro / nomes dos modais e manda email para mim
		$.ajax({ 	url: document.location.origin + '/manual/registraErroModal.php',
					data: {pergunta: ultimaPergunta, tipoContrato: window.location.href},
					type: 'POST',
					success: function(output) {
			}
		});
	}


	if ( modalsAbertos < 1 ){
		// Grava em arquivo TXT o erro / nomes dos modais e manda email para mim
