function verificaDicaPergunta(proximaPergunta){
	var dica = perguntas.indexOf(proximaPergunta);
	if ( dica < 0 ){
	}else{
		dica = dicas[dica];
		// Verificar o tamanho da pergunta, se for maior que X caracteres muda a classe, para apresentar abaixo ou acima - Alinhar com o campo da pergunta
		// Ser mais fino também dependendo da largura?
		document.getElementById("dicaPergunta").className = "dicaPergunta";
		document.getElementById('textoDicaPergunta').innerText = dica;
		document.getElementById('pDicaPergunta').style.height = '';
	}

	try{
		if ( screen.height < screen.width ){
			var posicao = posicoesPerguntas.indexOf(proximaPergunta);
			if ( posicao < 0 ){
			}else{
				var offset = -200;
				posicao = posicoesPrevia[posicao];
				$('html, body').animate({
					scrollTop: $("#" + posicao).offset().top + offset
				}, 1000);
			}
		}
	}catch(err){
	}
}

