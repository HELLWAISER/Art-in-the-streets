			}else{
				targetDiv.focus();
			}
		}
	}catch(err) {
	}

	// Para não modificar o Array original
	var arr_temporario = arvorePerguntas.slice().sort();
	var results = [];
	for (var i = 0; i < arr_temporario.length - 1; i++) {
		if (arr_temporario[i] == proxima_pergunta) {
			// Grava em arquivo TXT o erro / nomes dos modais e manda email para mim
			$.ajax({ 	url: document.location.origin + '/manual/registraErroPerguntaDuplicada.php',
