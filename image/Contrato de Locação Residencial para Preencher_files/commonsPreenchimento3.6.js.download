// Registra em log todos erros apresentados por Javascript	
window.onerror = function(message, url, lineNumber, column, errorObj) {
	var browser = 'nao detectado';
	try{	
		browser = navigator.userAgent;
		if ( (!!window.opr && !!opr.addons) || !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0 ){
			browser = 'opera';
		}else if ( typeof InstallTrigger !== 'undefined' ){
			browser = 'firefox';
		}else if ( /constructor/i.test(window.HTMLElement) || (function (p) { return p.toString() === "[object SafariRemoteNotification]"; })(!window['safari'] || (typeof safari !== 'undefined' && safari.pushNotification)) ){
			browser = 'safari';
		}else if ( /*@cc_on!@*/false || !!document.documentMode ){
			browser = 'IE';
		}else if ( browser != 'IE' && !!window.StyleMedia ){
			browser = 'edge';
		}else if ( !!window.chrome && !!window.chrome.webstore ){
			browser = 'chrome';
		}else if ((browser == 'chrome' || browser == 'opera') && !!window.CSS){
			browser = 'blink';
		}
	}catch(err) {
	}

	var mensagem = '' +  '_' + message;
	if ( mensagem.indexOf('getElementsByClassName.ToString') == -1 ){
		if ( mensagem.indexOf('find variable: clipper') == -1 ){
			if ( mensagem.indexOf('Client is not defined') == -1 ){
				if ( mensagem.indexOf('ga is not defined') == -1 ){
					if ( mensagem.indexOf('pagead2') == -1 ){
						if ( mensagem.indexOf('adsbygoogle') == -1 ){
							if ( mensagem.indexOf('Script error.') == -1 ){
								var endereco = "" + window.location;
								if (endereco.toLowerCase().indexOf("www.99contratos.com.br") >= 0){								
									$.ajax({ 	url: document.location.origin + '/manual/registraErroJavascript.php',
												data: {dados: message + ' url erro: ' + url + ' | linha: ' + lineNumber + ' | Endereco browser: ' + window.location.href + ' | Resolucao: ' + screen.width + 'x' + screen.height + ' | Browser: ' + browser},
												type: 'POST',
												success: function(output) {
										}
									});
								}
							}
						}
					}
				}
			}
		}
	}
	return true;
}

function verificaEnterNumericoSemDigito(e, perguntaAtual, proximaPergunta) {
	if (e.keyCode == 13) {
		proxima_pergunta(perguntaAtual, proximaPergunta);
	}
	if ( e.charCode >= 48 && e.charCode <= 57 ){
		return true;
	}else{
		return false;
	}
}

function verificaEnterNumericoComDigito(e, perguntaAtual, proximaPergunta) {
	if (e.keyCode == 13) {
		proxima_pergunta(perguntaAtual, proximaPergunta);
	}
	if ( e.charCode == 46 || e.charCode == 44 || (e.charCode >= 48 && e.charCode <= 57 ) ){
		return true;
	}else{
		return false;
	}
}

function verificaNumericoConcluir(e) {
	if ( e.charCode == 46 || e.charCode == 44 || (e.charCode >= 48 && e.charCode <= 57 ) ){
		return true;
	}
	if (e.keyCode == 13) {
		concluirContrato();
	}
}			

function varificaLinhasTextArea(areaId) {
	var inTxt = document.getElementById(areaId).value;
	var numberOfLineBreaks = (inTxt.match(/\n/g)||[]).length;
	var characterCount = inTxt.length + numberOfLineBreaks;
	document.getElementById(areaId).rows = numberOfLineBreaks + 2;
	var stringInputada = document.getElementById(areaId).value;
	if (stringInputada.indexOf("______") !=-1) {
		document.getElementById(areaId).value = '';
		alteraTamanhoInput(areaId);
	}
}

